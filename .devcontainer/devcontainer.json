{
    "name": "base-images",
    "dockerComposeFile": [
      "./docker-compose.yml"
    ],
    "service": "devcontainer-base-images",
    "remoteUser": "vscode",
    "shutdownAction": "stopCompose",
    "customizations": {
      "vscode": {
        "settings": {
          "terminal.integrated.defaultProfile.linux": "zsh"
        },
      "extensions": []
      }
    },
    "workspaceFolder": "/home/vscode/base-images",
    "features": {
        "ghcr.io/antyung/devcontainers/common-utils:1": {},
        "ghcr.io/antyung/devcontainers/docker-in-docker:1": {}
    }
}
